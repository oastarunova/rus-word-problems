encoding "utf8";

metric "длина"
{
    key = "м" | "метр" | "см" | "сантиметр" | "мм" | "миллиметр" | "дм" | "дециметр" 
    lemma = "длина"
}
metric "расстояние"
{
    key = "км" | "километр"
    lemma = "расстояние"
}
metric "вес" 
{
    key = "т" | "тонна" | "ц" | "центнер" | "кг" | "килограмм" | "г" | "грамм" 
    lemma = "вес"
}
metric "цена" 
{
    key = "р ." | "рубль" | "к ." | "!коп ." | "копейка"
    lemma = "цена"
}
metric "скорость" 
{
    key = "км / ч" | "километр в час" | "м / с" | "метр в секунду" 
    lemma = "скорость"
}
metric "площадь"
{
    key = "м 2" | "кв . м" | "квадратный метр" | "га" | "гектар"
    lemma = "площадь"
}
metric "время"
{
    key = "сутки" | "день" | "ч" | "час" | "!мин" | "минута" | "!сек" | "секунда" | "месяц" | "год" | "неделя"
    lemma = "время"
}
metric "объем"
{
    key = "л" | "литр" | "!куб . м" | "кубический метр" | "кубометр" | "!куб . см" | "кубический сантиметр"
    lemma = "объем"
}
metric "количество"
{
    key = "штука" | "шт ." | "человек" | "!чел ." | "голова"
    lemma = "количество"
}
dimension "размерность"
{
    key = "число" | "количество" | "скорость" | "расстояние" | "время" | "длина" | "ширина" | "высота" | "глубина" | "цена" | "стоимость" | "раз" | "путь" | "деньги" | "масса" | "площадь" | "часть" | "объем" | "процент" | "продолжительность" | "направление" | "дробь" | "делитель" | "уменьшаемое" | "вес" 
}
dimension "абстракции"
{
    key = { "abstract.txt" type=FILE } 
}
